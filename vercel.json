{
    "version": 2,
    "buildCommand": "pnpm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "headers": [
        {
            "source": "/assets/:path*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                }
            ]
        },
        {
            "source": "/_vercel/:path*",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        },
        {
            "source": "/:path*",
            "headers": [
                {
                    "key": "Strict-Transport-Security",
                    "value": "max-age=31536000; includeSubDomains"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                },
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src 'self' *.vercel.app *.vercel.com; script-src 'self' 'unsafe-inline' 'unsafe-eval' *.vercel.app *.vercel.com va.vercel-scripts.com speed-insights.vercel.app vitals.vercel-insights.com; style-src 'self' 'unsafe-inline'; img-src 'self' data: blob: https:; font-src 'self' data:; connect-src 'self' *.vercel.app *.vercel.com *.therapytrain.ai va.vercel-scripts.com vitals.vercel-insights.com speed-insights.vercel.app; worker-src 'self' blob:; frame-src 'self' *.vercel.app *.vercel.com; manifest-src 'self';"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/_vercel/insights/script.js",
            "destination": "https://va.vercel-scripts.com/v1/script.js"
        },
        {
            "source": "/_vercel/speed-insights/script.js",
            "destination": "https://speed-insights.vercel.app/script.js"
        },
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ],
    "env": {
        "VITE_APP_TITLE": "TherapyTrain AI",
        "VITE_APP_ENV": "production",
        "NODE_ENV": "production"
    }
}