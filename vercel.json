{
    "version": 2,
    "buildCommand": "pnpm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "headers": [
        {
            "source": "/assets/:path*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                }
            ]
        },
        {
            "source": "/_vercel/:path*",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        },
        {
            "source": "/:path*",
            "headers": [
                {
                    "key": "Strict-Transport-Security",
                    "value": "max-age=31536000; includeSubDomains"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                },
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src 'self' https:; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://*.vercel.app https://*.vercel.com https://va.vercel-scripts.com https://speed-insights.vercel.app https://vitals.vercel-insights.com; style-src 'self' 'unsafe-inline' https: data:; img-src 'self' data: blob: https: *; font-src 'self' data: https: *; connect-src 'self' https://*.vercel.app https://*.vercel.com https://*.therapytrain.ai https://va.vercel-scripts.com https://vitals.vercel-insights.com https://speed-insights.vercel.app wss: *; worker-src 'self' blob:; frame-src 'self' https://*.vercel.app https://*.vercel.com; manifest-src 'self'; base-uri 'self'; object-src 'none'; media-src 'self' https: data:; form-action 'self';"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/_vercel/insights/script.js",
            "destination": "https://va.vercel-scripts.com/v1/script.js"
        },
        {
            "source": "/_vercel/speed-insights/script.js",
            "destination": "https://speed-insights.vercel.app/script.js"
        },
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ],
    "env": {
        "VITE_APP_TITLE": "TherapyTrain AI",
        "VITE_APP_ENV": "production",
        "NODE_ENV": "production"
    }
}